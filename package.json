{
  "name": "urban-scraper",
  "version": "2.0.2",
  "license": "MIT",
  "author": "brunolepis",
  "keywords": [
    "scraper",
    "scrape",
    "urban-dictionary",
    "urban"
  ],
  "main": "dist/index.js",
  "module": "dist/urban-scraper.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node",
    "globalSetup": "./global-setup.js"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/urban-scraper.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/urban-scraper.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/esbuild": "^11.1.5",
    "@size-limit/esbuild-why": "^11.1.5",
    "@size-limit/preset-small-lib": "^11.1.5",
    "@tsconfig/recommended": "^1.0.7",
    "@types/jest": "^29.5.13",
    "@types/node-fetch": "^2.6.11",
    "dts-cli": "^2.0.5",
    "husky": "^9.1.6",
    "size-limit": "^11.1.5",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "node-fetch": "2"
  }
}
